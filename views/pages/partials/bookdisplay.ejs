<main class="resultsMain">

        <% book.forEach(book => { %>
    <article class="bookRender">
        <img src="<%= book.image %>" >
        
        <h2><%= book.title %></h2> 
        
        <h3><%= book.author %></h3>
        <div class="book-synopsis-container">
           <p>  <%= book.description %>    </p>  
        </div>
         
        <h3> ISBN: <%= book.isbn %> </h3>
        <h3> Bookshelf: <%= book.bookshelf %> </h3>
        
        
    </article>
        <% }) %>

</main>