<main class="resultsMain">

        <% book.forEach(book => { %>
    <article class="bookRender">
        <img src="<%= book.image %>" >
        
        <h2><%= book.title %></h2> 
        
        <h3><%= book.author %></h3>
        
        <p>  <%= book.description %>    </p>  
        
        
    </article>
        <% }) %>

</main>