<main class="resultsMain">

    <% book.forEach(book => { %>
<article class="bookRender">
    <img src="<%= book.image %>" >
    
    <h2><%= book.title %></h2> 
    
    <h3><%= book.author %></h3>
    
    <p>  <%= book.description %>    </p>  
    

    <form class="addBookForm" action="/books" method="POST" >
        <fieldset class="addBookField">
    
            <input type="text" name="title" value="<%= book.title %>">
            <input type="text" name="author" value="<%= book.author %>">
            <input type="text" name="description" value="<%= book.description %>">
            <input type="text" name="isbn" value="<%= book.isbn %>">
            <input type="text" name="bookshelf" value="<%= book.bookshelf %>">
            <input type="text" name="image" value="<%= book.image %>">
        </fieldset>
            <button class="submitButton" type="submit">Add to Collection</button>
    </form>

    
</article>
    <% }) %>

</main>